  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A markup highlighting module for the Quill text editor",
  "main": "./public/bundle.js",
  "types": "./index.d.ts",
  "private": false,
  "debug": false,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/quill-markup"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/quill-markup/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.2",
    "@types/highlight.js": "^9.12.2",
    "@types/jsdom": "^11.0.4",
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.6.6",
    "@types/quill": "^1.3.6",
    "@types/sinon": "^4.3.1",
    "@types/xregexp": "^3.0.29",
    "ava": "^0.25.0",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-minify": "^0.4.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.4.0",
    "browser-env": "^3.2.5",
    "ccount": "^1.0.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "fs-extra": "^5.0.0",
    "hash.js": "^1.1.3",
    "highlight.js": "^9.12.0",
    "js-output-loader": "^0.0.5",
    "jsdom": "^11.9.0",
    "lodash": "^4.17.10",
    "lorem-ipsum": "^1.0.4",
    "mini-css-extract-plugin": "^0.4.0",
    "mock-css-modules": "^1.0.0",
    "nyc": "^11.7.1",
    "parchment": "^1.1.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-modules": "^1.1.0",
    "precss": "^3.1.2",
    "process-nextick-args": "^2.0.0",
    "quill": "^1.3.6",
    "shelljs": "^0.8.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.21.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "util.ds": "^0.0.38",
    "util.fixture": "0.0.63",
    "util.fontlist": "^0.0.11",
    "util.join": "^0.0.19",
    "util.leader": "^0.0.38",
    "util.matches": "^0.0.8",
    "util.pkgwrap": "^0.0.92",
    "util.rstrip": "^0.0.35",
    "util.section": "^0.0.11",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "xregexp": "^4.1.1"
  },
  "pkgwrap": {
    "cleanup": [
      "public/bundle.js",
      "index.js",
      "index.d.ts",
      "server.js",
      "server.d.ts",
      "./lib/**/*.d.ts",
      "./lib/markup.js",
      "./lib/modes/**/*.js",
      "./lib/**/*.d.ts",
      "./test/helpers/index.js",
      "./test/highlight.test.js"
    ]
  },
  "ava": {
    "babel": "inherit"
  },
  "scripts": {
    "all": "yarn install && cross-env NODE_ENV=production yarn run build && yarn test && yarn run report",
    "build": "yarn clean && yarn run build:highlights && yarn run lint && pkgwrap --build --webpack",
    "build:highlights": "node bin/buildHighlights.js",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "start": "node server.js",
    "test": "pkgwrap --testing --ava",
    "watch": "webpack --watch --progress"
  }
}
