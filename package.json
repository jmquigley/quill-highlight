{
  "name": "quill-markup",
  "version": "0.0.1",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A markup highlighting module for the Quill text editor",
  "private": false,
  "debug": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/quill-markup"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/quill-markup/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "parchment": "^1.1.0",
    "quill": "^1.3.2",
    "util.join": "^0.0.14",
    "util.matches": "^0.0.2",
    "util.pkgwrap": "0.0.68",
    "util.rstrip": "^0.0.26",
    "util.section": "^0.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/fs-extra": "^4.0.2",
    "@types/highlight.js": "^9.1.10",
    "@types/jsdom": "^11.0.2",
    "@types/node": "^8.0.28",
    "@types/quill": "^1.3.3",
    "ava": "^0.22.0",
    "awesome-typescript-loader": "^3.2.3",
    "babel-minify": "^0.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-minify": "^0.2.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.7",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.2",
    "js-output-loader": "^0.0.5",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "lorem-ipsum": "^1.0.4",
    "mock-css-modules": "^1.0.0",
    "nyc": "^11.2.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules": "^0.8.0",
    "precss": "^2.0.0",
    "process-nextick-args": "^1.0.7",
    "style-loader": "^0.18.2",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "util.fixture": "0.0.58",
    "webpack": "^3.6.0"
  },
  "main": "./public/bundle.js",
  "types": "./index.d.ts",
  "scripts": {
    "all": "yarn install && yarn run clean && yarn run lint && yarn run build && yarn test && yarn run report",
    "build": "pkgwrap --build --webpack",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "postinstall": "pkgwrap --postinstall",
    "report": "pkgwrap --reporting",
    "start": "node server.js",
    "test": "pkgwrap --testing --ava",
    "watch": "webpack --watch --progress"
  },
  "pkgwrap": {
    "cleanup": [
      "public/bundle.js",
      "index.js",
      "index.d.ts",
      "server.js",
      "server.d.ts",
      "./lib/**/*.d.ts",
			"./lib/markup.js",
			"./lib/modes/**/*.js",
      "./lib/**/*.d.ts",
      "./test/helpers/index.js",
      "./test/highlight.test.js"
    ]
  },
  "ava": {
    "babel": "inherit"
  }
}
