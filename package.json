{
  "name": "quill-markup",
  "version": "0.0.26",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A markup highlighting module for the Quill text editor",
  "main": "./public/bundle.js",
  "types": "./index.d.ts",
  "private": false,
  "debug": false,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/quill-markup"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/quill-markup/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime": "^7.2.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^23.3.12",
    "@types/jsdom": "^12.2.1",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/quill": "^2.0.1",
    "@types/sinon": "^7.0.3",
    "@types/xregexp": "^3.0.29",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-minify": "^0.5.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-minify": "^0.5.0",
    "browser-env": "^3.2.5",
    "ccount": "^1.0.3",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "hash.js": "^1.1.7",
    "highlight.js": "^9.13.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "js-output-loader": "^0.0.5",
    "jsdom": "^13.1.0",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^1.0.6",
    "mini-css-extract-plugin": "^0.5.0",
    "mock-css-modules": "^1.0.0",
    "parchment": "^1.1.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.5.0",
    "precss": "^4.0.0",
    "process-nextick-args": "^2.0.0",
    "quill": "^1.3.6",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-babel-jest": "^1.0.5",
    "util.ds": "^0.0.39",
    "util.fixture": "0.0.65",
    "util.fontlist": "^0.0.12",
    "util.join": "^0.0.21",
    "util.leader": "^0.0.40",
    "util.matches": "^0.0.9",
    "util.pkgwrap": "^0.0.94",
    "util.rstrip": "^0.0.40",
    "util.section": "^0.0.12",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "xregexp": "^4.2.4"
  },
  "pkgwrap": {
    "cleanup": [
      "public/bundle.js",
      "index.js",
      "index.d.ts",
      "server.js",
      "server.d.ts",
      "./lib/**/*.d.ts",
      "./lib/markup.js",
      "./lib/modes/**/*.js",
      "./lib/**/*.d.ts"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|style)$": "identity-obj-proxy"
    },
    "notify": false,
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/helpers"
    ],
    "verbose": false
  },
  "scripts": {
    "all": "yarn install && cross-env NODE_ENV=production yarn run build && yarn test",
    "build": "yarn clean && yarn run build:highlights && yarn run lint && pkgwrap --build --webpack",
    "build:highlights": "node bin/buildHighlights.js",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage --jest",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "start": "node server.js",
    "test": "pkgwrap --testing --jest",
    "watch": "webpack --watch --progress"
  }
}
