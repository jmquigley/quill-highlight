{
  "name": "quill-highlight",
  "version": "0.0.3",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A highlighting module for the Quill text editor",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/quill-highlight"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/quill-highlight/issues"
  },
  "engines": {
    "node": ">=7.5"
  },
  "dependencies": {
    "quill": "^1.3.2",
    "util.join": "^0.0.14",
    "util.pkgwrap": "0.0.68"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/fs-extra": "^4.0.2",
    "@types/jsdom": "^11.0.2",
    "@types/node": "^8.0.28",
    "ava": "^0.22.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.1",
    "express": "^4.15.4",
    "fs-extra": "^4.0.2",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "lorem-ipsum": "^1.0.4",
    "nyc": "^11.2.1",
    "style-loader": "^0.17.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "util.fixture": "0.0.56",
    "webpack": "^3.5.6"
  },
  "main": "./public/bundle.js",
  "types": "./index.d.ts",
  "scripts": {
    "all": "yarn install && yarn run clean && yarn run lint && yarn run build && yarn test && yarn run report",
    "build": "pkgwrap --build --webpack",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "postinstall": "pkgwrap --postinstall",
    "report": "pkgwrap --reporting",
    "start": "node server.js",
    "test": "pkgwrap --testing --ava"
  },
  "pkgwrap": {
    "cleanup": [
      "public/bundle.js"
    ]
  },
  "ava": {
    "babel": "inherit"
  }
}
