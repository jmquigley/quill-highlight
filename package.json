{
  "name": "quill-markup",
  "version": "0.0.22",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A markup highlighting module for the Quill text editor",
  "main": "./public/bundle.js",
  "types": "./index.d.ts",
  "private": false,
  "debug": false,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/quill-markup"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/quill-markup/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/fs-extra": "^4.0.5",
    "@types/highlight.js": "^9.12.2",
    "@types/jsdom": "^11.0.4",
    "@types/lodash": "^4.14.87",
    "@types/node": "^8.0.54",
    "@types/quill": "^1.3.3",
    "@types/sinon": "^4.0.0",
    "@types/xregexp": "^3.0.29",
    "ava": "^0.24.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify": "^0.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "browser-env": "^3.2.4",
    "ccount": "^1.0.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "hash.js": "^1.1.3",
    "highlight.js": "^9.12.0",
    "js-output-loader": "^0.0.5",
    "jsdom": "^11.5.1",
    "lodash": "^4.17.4",
    "lorem-ipsum": "^1.0.4",
    "mock-css-modules": "^1.0.0",
    "nyc": "^11.3.0",
    "parchment": "^1.1.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-modules": "^1.1.0",
    "precss": "^2.0.0",
    "process-nextick-args": "^1.0.7",
    "quill": "^1.3.4",
    "shelljs": "^0.7.8",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "util.ds": "^0.0.35",
    "util.fixture": "0.0.61",
    "util.fontlist": "^0.0.8",
    "util.join": "^0.0.17",
    "util.leader": "^0.0.36",
    "util.matches": "^0.0.6",
    "util.pkgwrap": "^0.0.88",
    "util.rstrip": "^0.0.32",
    "util.section": "^0.0.9",
    "webpack": "^3.9.1",
    "xregexp": "^3.2.0"
  },
  "pkgwrap": {
    "cleanup": [
      "public/bundle.js",
      "index.js",
      "index.d.ts",
      "server.js",
      "server.d.ts",
      "./lib/**/*.d.ts",
      "./lib/markup.js",
      "./lib/modes/**/*.js",
      "./lib/**/*.d.ts",
      "./test/helpers/index.js",
      "./test/highlight.test.js"
    ]
  },
  "ava": {
    "babel": "inherit"
  },
  "scripts": {
    "all": "yarn install && cross-env NODE_ENV=production yarn run build && yarn test && yarn run report",
    "build": "yarn clean && yarn run build:highlights && yarn run lint && pkgwrap --build --webpack",
    "build:highlights": "node bin/buildHighlights.js",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "start": "node server.js",
    "test": "pkgwrap --testing --ava",
    "watch": "webpack --watch --progress"
  }
}
